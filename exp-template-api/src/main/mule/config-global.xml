			<apikit:flow-mapping resource="/exp-menu" action="get" flow-ref="get:\exp-menu:exp-template-api-config" />
			<apikit:flow-mapping resource="/exp-menu" action="post" content-type="application/json" flow-ref="post:\exp-menu:application\json:exp-template-api-config" />
		</apikit:flow-mappings>
