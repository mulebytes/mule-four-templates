http://www.mulesoft.org/schema/mule/secure-properties http://www.mulesoft.org/schema/mule/secure-properties/current/mule-secure-properties.xsd">
	<apikit:config outboundHeadersMapName="outboundHeadersMapName" httpStatusVarName="httpStatus" doc:name="Router" doc:id="78cb6a82-6070-463f-b34d-b9c383e495ef" name="Router" api="resource::e3a6d836-43b9-49f5-8c12-c8e2c4c61e87:exp-template-api:1.0.0:raml:zip:exp-template-api.raml" disableValidations="true">
